                    cube::metric::set::mpi_file_crops("value", "VOID");
                    cube::metric::set::mpi_file_cwops("value", "VOID");
                    cube::metric::set::bytes("value", "VOID");
                    cube::metric::set::bytes_p2p("value", "VOID");
                    cube::metric::set::bytes_sent("value", "VOID");
